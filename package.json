{
  "name": "spindb",
  "version": "0.2.02",
  "description": "Spin up local database containers without Docker. A DBngin-like CLI for PostgreSQL.",
  "type": "module",
  "bin": {
    "spindb": "./bin/cli.js"
  },
  "scripts": {
    "start": "tsx src/bin/cli.ts",
    "dev": "tsx watch src/bin/cli.ts",
    "build": "tsc",
    "test": "tsx --test",
    "format": "prettier --write .",
    "lint": "eslint ."
  },
  "keywords": [
    "postgres",
    "postgresql",
    "database",
    "local",
    "development",
    "cli"
  ],
  "author": "Bob Bass <bob@bbass.co>",
  "license": "MIT",
  "packageManager": "pnpm@9.14.2",
  "engines": {
    "node": ">=18"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/robertjbass/spindb.git"
  },
  "homepage": "https://github.com/robertjbass/spindb#readme",
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "inquirer": "^9.3.7",
    "ora": "^8.1.1",
    "tsx": "^4.7.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/inquirer": "^9.0.7",
    "@types/node": "^20.10.0",
    "eslint": "^9.39.1",
    "prettier": "^3.6.2",
    "typescript": "^5.3.0",
    "typescript-eslint": "^8.48.0"
  },
  "tsx": {
    "tsconfig": "./tsconfig.json"
  },
  "files": [
    "bin/**/*",
    "src/**/*",
    "tsconfig.json"
  ]
}
