{
  "name": "spindb",
  "version": "0.19.2",
  "description": "Zero-config Docker-free local database containers. Create, backup, and clone a variety of popular databases.",
  "type": "module",
  "bin": {
    "spindb": "./bin/cli.js"
  },
  "scripts": {
    "start": "tsx cli/bin.ts",
    "dev": "tsx watch cli/bin.ts",
    "test": "run-s test:all",
    "test:pg": "node --import tsx --test --experimental-test-isolation=none tests/integration/postgresql.test.ts",
    "test:mysql": "node --import tsx --test --experimental-test-isolation=none tests/integration/mysql.test.ts",
    "test:mariadb": "node --import tsx --test --experimental-test-isolation=none tests/integration/mariadb.test.ts",
    "test:sqlite": "node --import tsx --test --experimental-test-isolation=none tests/integration/sqlite.test.ts",
    "test:mongodb": "node --import tsx --test --experimental-test-isolation=none tests/integration/mongodb.test.ts",
    "test:redis": "node --import tsx --test --experimental-test-isolation=none tests/integration/redis.test.ts",
    "test:valkey": "node --import tsx --test --experimental-test-isolation=none tests/integration/valkey.test.ts",
    "test:clickhouse": "node --import tsx --test --experimental-test-isolation=none tests/integration/clickhouse.test.ts",
    "test:duckdb": "node --import tsx --test --experimental-test-isolation=none tests/integration/duckdb.test.ts",
    "test:unit": "node --import tsx --test --experimental-test-isolation=none tests/unit/*.test.ts",
    "test:cli": "node --import tsx --test --experimental-test-isolation=none tests/integration/cli-e2e.test.ts",
    "test:hostdb-sync": "node --import tsx --test --experimental-test-isolation=none tests/integration/hostdb-sync.test.ts",
    "test:integration": "run-s test:pg test:mysql test:mariadb test:sqlite test:duckdb test:mongodb test:redis test:valkey test:clickhouse",
    "test:all": "run-s test:unit test:integration",
    "test:local": "./scripts/test-local.sh",
    "test:local:quick": "./scripts/test-local.sh --quick",
    "test:local:fresh": "./scripts/test-local.sh --fresh",
    "test:docker": "bash tests/docker/run-docker-test.sh",
    "format": "prettier --write .",
    "lint": "tsc --noEmit && eslint .",
    "prepare": "husky"
  },
  "keywords": [
    "hostdb",
    "spindb",
    "postgres",
    "postgresql",
    "mysql",
    "mariadb",
    "database",
    "local",
    "development",
    "mongodb",
    "redis",
    "valkey",
    "clickhouse",
    "ferretdb",
    "sqlite",
    "duckdb",
    "tui",
    "cli"
  ],
  "author": "Bob Bass <bob@bbass.co>",
  "license": "PolyForm-Noncommercial-1.0.0",
  "packageManager": "pnpm@9.14.2",
  "engines": {
    "node": ">=18",
    "pnpm": ">=8"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/robertjbass/spindb.git"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "homepage": "https://github.com/robertjbass/spindb#readme",
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "inquirer": "^9.3.7",
    "ora": "^8.1.1",
    "string-width": "^8.1.0",
    "tsx": "^4.7.0",
    "unzipper": "^0.12.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/inquirer": "^9.0.7",
    "@types/node": "^20.10.0",
    "@types/unzipper": "^0.10.11",
    "eslint": "^9.39.1",
    "husky": "^9.1.7",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.6.2",
    "typescript": "^5.3.0",
    "typescript-eslint": "^8.48.0"
  },
  "files": [
    "bin",
    "cli",
    "core",
    "config",
    "engines",
    "types"
  ]
}
